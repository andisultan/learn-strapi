{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "weight": {
      "type": "integer"
    },
    "qty": {
      "type": "integer"
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "price_coret": {
      "type": "integer"
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
